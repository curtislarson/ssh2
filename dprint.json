{
  "lineWidth": 120,
  "indentWidth": 2,
  "newLineKind": "auto",
  "useTabs": false,
  "incremental": true,
  "typescript": {
    "arrowFunction.useParentheses": "force",
    "quoteStyle": "alwaysDouble"
  },
  "json": {},
  "markdown": {
    "lineWidth": 80
  },
  "dockerfile": {},
  "includes": ["**/*.{ts,tsx,jsx,cjs,mjs,mts,cts,json,md,mdx,yml,yaml,jsonc,dockerfile,conf}"],
  "excludes": [
    "**/node_modules",
    "**/*-deno.lock",
    "**/node",
    "**/.deno",
    "**/vendor",
    "**/packages-experiments/**",
    "**/dist",
    "**/.gen",
    "pnpm-lock.yaml",
    "pnpm-workspace.yaml",
    "deno.lock"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.71.2.wasm",
    "https://plugins.dprint.dev/json-0.15.5.wasm",
    "https://plugins.dprint.dev/markdown-0.14.0.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.0.wasm",
    "https://plugins.dprint.dev/prettier-0.11.0.json@385edfc0e1212b3be6412cda49322ecd62cb38ac71b9b463869bebf23a4767e3"
  ]
}
